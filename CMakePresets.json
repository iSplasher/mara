{
    "version": 2,
    "configurePresets": [
        {
            "name": "default-debug",
            "displayName": "Clang 13.0.1 (MSVC CLI) for MSVC 17.2.32616.157 (Visual Studio Community 2022 Release - x64)",
            "description": "Using compilers: C = C:\\Program Files\\Microsoft Visual Studio\\2022\\Community\\VC\\Tools\\Llvm\\bin\\clang-cl.exe, CXX = C:\\Program Files\\Microsoft Visual Studio\\2022\\Community\\VC\\Tools\\Llvm\\bin\\clang-cl.exe",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "generator": "Ninja",
            "architecture": {
                "strategy": "external",
                "value": "x64"
            },
            "toolset": {
                "strategy": "external",
                "value": "x64"
            },
            "cacheVariables": {
                "VCPKG_TARGET_TRIPLET": "x64-windows",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/build/install/${presetName}",
                "VCPKG_PREFER_SYSTEM_LIBS": {
                    "type": "BOOL",
                    "value": "False"
                },
                "CMAKE_C_COMPILER": "clang-cl.exe",
                "CMAKE_CXX_COMPILER": "clang-cl.exe",
                "CMAKE_EXPORT_COMPILE_COMMANDS": "1",
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_TOOLCHAIN_FILE": {
                    "value": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
                    "type": "FILEPATH"
                }
            }
        }
    ],
    "buildPresets": [
        {
            "name": "Debug",
            "configurePreset": "default-debug"
        }
    ],
    "testPresets": [
        {
            "name": "Test",
            "configurePreset": "default-debug"
        }
    ]
}