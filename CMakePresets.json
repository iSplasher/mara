{
    "version": 2,
    "configurePresets": [
        {
            "name": "default-debug",
            "displayName": "Clang 9.0.0 (MSVC CLI) (Visual Studio Enterprise 2019 Release - x86)",
            "description": "Using compilers: C = C:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\Enterprise\\VC\\Tools\\Llvm\\bin\\clang-cl.exe, CXX = C:\\Program Files (x86)\\Microsoft Visual Studio\\2019\\Enterprise\\VC\\Tools\\Llvm\\bin\\clang-cl.exe",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "generator": "Ninja",
            "architecture": {
                "strategy": "external",
                "value": "x86"
            },
            "toolset": {
                "strategy": "external",
                "value": "x86"
            },
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "clang-cl.exe",
                "CMAKE_CXX_COMPILER": "clang-cl.exe",
                "CMAKE_EXPORT_COMPILE_COMMANDS": "1",
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_TOOLCHAIN_FILE": {
                    "value": "D:/Code/vcpkg/scripts/buildsystems/vcpkg.cmake",
                     "type": "FILEPATH"
                  }
            }
            
        }
    ],
    "buildPresets": [
        {
            "name": "Debug",
            "configurePreset": "default-debug"
        }
    ],
    "testPresets": [
        {
            "name": "Test",
            "configurePreset": "default-debug"
        }
    ]
}